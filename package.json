  "license": "ISC",
  "homepage": "https://bitbucket.org/Quinck/collections#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chai": "^4.3.6",
    "eslint": "^8.7.0",
    "mocha": "^9.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "files": [
    "lib/**/*"
  ]
