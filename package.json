{
  "name": "@quinck/collections",
  "version": "0.0.3",
  "description": "Allows extra operations on JavaScript collections: Array, Map and Set.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint src/ __tests__/",
    "lint:fix": "eslint src/ __tests__/ --fix",
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && npm run git-tag && git push --tags",
    "git-tag": "PACKAGE_VERSION=$(node -p -e \"require('./package.json').version\") && git tag v$PACKAGE_VERSION"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/Quinck/collections.git"
  },
  "keywords": [
    "Collection",
    "Array",
    "Set",
    "Map"
  ],
  "author": "Quinck s.r.l.",
  "license": "ISC",
  "homepage": "https://bitbucket.org/Quinck/collections#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "files": [
    "lib/**/*"
  ]
}
